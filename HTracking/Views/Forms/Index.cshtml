@model IList<HTracking.Models.BaseForm>

@{
    ViewData["Title"] = "Forms";
}
<h1>@ViewBag.Title</h1>
<a asp-controller="Forms" asp-action="NewSiteForm">Submit Site Form</a>
<a asp-controller="Forms" asp-action="NewInventoryForm">Submit Inventory Form</a>
<table>
    <tr>
        <td>Id</td>
        <td>Name</td>
    </tr>
    @for(var i = 0; i< @Model.Count; i++) {
        <tr>
            <td>@Model[i].Id</td>
            <td>@Model[i].Name</td>
        </tr>
    }
    
</table>
<p>
    Forms found: @Model.Count
</p>